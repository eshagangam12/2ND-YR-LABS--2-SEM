import java.util.*;
public class Main
{
	public static void main(String[] args) {
	    char[] ch = {'e','s','h','a'};
	    String s = new String(ch);
		System.out.println(s);
		
		String str = "esha";
		char[] cha = new char[str.length()];
		for(int i = 0;i<str.length();i++)
		{
		    cha[i] = str.charAt(i);
		}
		for(char c:cha)
		{
		    System.out.println(c);
		}
	}
}
